  "query": "SELECT media.id, media.library_id, media.name, description, rating as \"rating: _\", year, added, poster_path, backdrop_path, media.media_type as \"media_type: _\"\n                FROM media\n                JOIN library ON library.id = media.library_id\n                WHERE NOT media.media_type = \"episode\" AND NOT library.hidden\n                AND UPPER(media.name) LIKE ?\n                LIMIT ?\n                ",
